{
  "name": "cli-driver",
  "version": "0.1.0",
  "description": "like webdriver but for the command line",
  "main": "lib/src/index.js",
  "types": "types/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "npm run test-critical",
    "test-critical": "ts-node -r tsconfig-paths/register node_modules/jasmine/bin/jasmine --config=./spec/support/jasmine-critical.json ",
    "test-all": "ts-node -r tsconfig-paths/register node_modules/jasmine/bin/jasmine --config=./spec/support/jasmine-all.json ",
    "test-coverage": "rm -rf coverage .ny_output; nyc  -r lcov -e .ts -x \"*.spec.ts\" ts-node -r tsconfig-paths/register node_modules/jasmine/bin/jasmine --config=./spec/support/jasmine-critical.json  ",
    "prepare": "tsc",
    "doc": "tsc && typedoc --options typedoc.js",
    "clean": "find . -name \"node_modules\" -exec rm -rf '{}' + ; rm -rf  package-lock.json lib tmp* src/*.d.ts",
    "lint": "tslint src/**/*.ts",
    "lint-fix": "tslint --fix src/**/*.ts"
  },
  "files": [
    "lib"
  ],
  "keywords": [],
  "author": "Sebasti√°n Gurin",
  "license": "ISC",
  "dependencies": {
    "ansi-escape-sequences": "git+https://github.com/cancerberoSgx/ansi-escape-sequences.git#types",
    "node-pty": "git+https://github.com/cancerberoSgx/node-pty.git",
    "shelljs": "^0.8.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.6.2",
    "@types/shelljs": "^0.7.8",
    "jasmine": "^3.1.0",
    "nyc": "^11.6.0",
    "strip-ansi": "^4.0.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.1"
  },
  "readme": "../../README.MD"
}